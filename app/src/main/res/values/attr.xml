<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--倒计时的样式-->
    <declare-styleable name="CountDownView">
        <!--颜色-->
        <attr name="ringColor" format="color" />
        <!-- 进度文本的字体大小 -->
        <attr name="progressTextSize" format="dimension" />
        <!-- 圆环宽度 -->
        <attr name="ringWidth" format="float" />
        <!--进度文本颜色-->
        <attr name="progressTextColor" format="color"/>
        <!--倒计时-->
        <attr name="countdownTime" format="integer"/>
    </declare-styleable>

    <!-- ViewPagerLineIndicator 指示器的样式 -->
    <declare-styleable name="ViewPagerLineIndicator">
        <attr name="line_color" format="color" />
        <attr name="item_count" format="integer" />
        <attr name="text_select" format="color" />
        <attr name="text_unselect" format="color" />
    </declare-styleable>

    <declare-styleable name="Wave">
        <attr name="color" format="color"/>
        <attr name="text" format="string"/>
    </declare-styleable>
</resources>