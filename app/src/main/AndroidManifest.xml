<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="liyihuan.app.android.androidpractice">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".rvlayout.MyRvActivity">

        </activity>
        <activity android:name=".lifecycle.LifeActivity"></activity>
        <activity android:name=".danmu.DanmuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".match.PlanetActivity" />
        <activity android:name=".imdemo.ImDemoActivity"/>
        <activity android:name=".camera.CameraDemo" />
        <activity android:name=".tarotview.TarotActivity" />
        <activity android:name=".Indicator.IndicatorActivity" />
        <activity android:name=".permission.PermissionActivity" />
        <activity android:name=".wechat.WechatActivity" />
        <activity android:name=".swipecard.SwipeCardActivity" />
        <activity android:name=".timecount.TimeCountActivity" />
        <activity android:name=".MainActivity" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="liyihuan.app.android.androidpractice.fileprovider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>
    </application>

</manifest>